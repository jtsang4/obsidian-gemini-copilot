{
	"name": "obsidian-gemini-copilot",
	"version": "3.3.2",
	"description": "Gemini Scribe, allows you to interact with the model and use your notes as context.",
	"main": "main.js",
	"types": "src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./main.js"
		}
	},
	"scripts": {
		"dev:build": "rslib build --watch",
		"dev:copy": "node copy-dist-main.mjs",
		"dev": "echo 'Run dev: two terminals -> pnpm dev:build and pnpm dev:copy' && exit 0",
		"build": "tsc -noEmit -skipLibCheck && rslib build && node -e \"require('fs').cpSync('dist/main.js','main.js',{force:true})\"",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"postversion": "git push && git push --follow-tags",
		"format": "biome format --write .",
		"format-check": "biome format --check .",
		"lint": "biome lint .",
		"lint-fix": "biome lint --write .",
		"check": "biome check --diagnostic-level=error .",
		"check-fix": "biome check --write .",
		"ci": "biome ci .",
		"test": "jest"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@google/genai": "^1.29.0",
		"@rslib/core": "^0.17.1",
		"@types/jest": "^29.5.14",
		"@types/node": "^16.18.126",
		"builtin-modules": "3.3.0",
		"codemirror-companion-extension": "^0.0.11",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"obsidian": "latest",
		"obsidian-dataview": "^0.5.68",
		"raw-loader": "^4.0.2",
		"svelte": "4.2.19",
		"ts-jest": "^29.4.5",
		"ts-loader": "^9.5.4",
		"tslib": "2.4.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"handlebars": "^4.7.8"
	}
}
